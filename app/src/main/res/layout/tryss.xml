<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home">


    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="New Button"
        android:id="@+id/button6"
        android:layout_marginBottom="99dp"
        android:background="@drawable/w3"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/imageButton3"
        android:layout_alignStart="@+id/imageButton3"
        android:onClick="OnClickw1"/>

    <ImageButton
        android:layout_width="100dp"
        android:onClick="OnClickw2"
        android:layout_height="100dp"
        android:text="New Button"
        android:id="@+id/button12"
        android:background="@drawable/w4"
        android:layout_alignTop="@+id/button6"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="100dp"
        android:onClick="OnClickw3"
        android:layout_height="100dp"
        android:text="New Button"
        android:background="@drawable/w7"
        android:id="@+id/button13"
        android:layout_alignTop="@+id/button12"
        android:layout_alignLeft="@+id/imageButton"
        android:layout_alignStart="@+id/imageButton" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton"
        android:background="@drawable/m2"
        android:layout_marginRight="30dp"
        android:layout_marginEnd="23dp"
        android:layout_above="@+id/button13"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="OnClickm2"/>

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton2"
        android:background="@drawable/m7"
        android:layout_alignWithParentIfMissing="false"
        android:layout_above="@+id/button12"
        android:layout_centerHorizontal="true"
        android:onClick="OnClickm3"/>

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton3"
        android:background="@drawable/m1"
        android:layout_above="@+id/button6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="30dp"
        android:onClick="OnClickm1"/>

</RelativeLayout>
